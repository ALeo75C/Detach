<div class="block products">
  <div class="pattern">
    <%#= image_tag(asset_path(@imagePath)) %>
  </div>
  <div class="content">
    <h2><%= @h2 %></h2>
    <div class="cardsCollection">
      <% @cla.all.sample(5).each do |el| %>
      <% @mod = el %>
      <%= render @cardType %>
      <% end %>
    </div>
    <%= render "but" %>
  </div>
</div>
